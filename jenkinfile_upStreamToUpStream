pipeline {
  agent any

  stages {
    
	stage ('UpStream_Job_1') {
      steps {
        echo "Triggering UpStream_Job_1"
        build job: 'UpStream_Job_1'
      }
    }
    stage ('UpStream_Job2') {
      steps {
        echo "Triggering UpStream_Job2"
        build job: 'UpStream_Job2'
      }
    }
		   
  }
}